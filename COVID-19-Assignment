    "def func(data):\n",
    "    data.columns=data.columns.str.replace(' ','_').str.replace('/','_')\n",
    "    for column in data.columns:\n",
    "        if data[column].dtype == 'object':\n",
    "            data[column] = data[column].str.replace(' ', '_')\n",
    "    return data\n",
    "dt=func(data)\n",
    "dt.sample(6)\n"
   ]
